      <%= form_for haiku do |f| %>
      <div class="form-group">
        <%= f.label :title, 'タイトル' %>
        <%= f.text_field :title, class: "form-control" %>
      </div>
      <div class="form-group">
        <%= f.label :body, '本文' %>
        <%= f.text_area :body, rows: 3, class: "form-control" %>
      </div>
      <%= f.submit class: 'btn btn-success' %>
      <% end %>